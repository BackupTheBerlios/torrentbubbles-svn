####### Compiler, tools and options

CC       = gcc
CXX      = g++
LEX      = flex
YACC     = yacc
CFLAGS   = -pipe -O2 -march=i586 -mcpu=i686 -fmessage-length=0 -Wall -g -Wall -W -O2 -march=i586 -mcpu=i686 -fmessage-length=0 -Wall -g -D_REENTRANT  -DQT_NO_DEBUG -DQT_THREAD_SUPPORT -DQT_SHARED -DQT_TABLET_SUPPORT
CXXFLAGS = -pipe -O2 -march=i586 -mcpu=i686 -fmessage-length=0 -Wall -g -Wall -W -O2 -march=i586 -mcpu=i686 -fmessage-length=0 -Wall -g -D_REENTRANT  -DQT_NO_DEBUG -DQT_THREAD_SUPPORT -DQT_SHARED -DQT_TABLET_SUPPORT
LEXFLAGS = 
YACCFLAGS= -d
INCPATH  = -I/usr/lib/qt3/mkspecs/default -I. -I. -Isrc -I/usr/include -I$(QTDIR)/include
LINK     = g++
LFLAGS   = 
LIBS     = $(SUBLIBS) -L/usr/lib/ -L$(QTDIR)/lib/ -L/usr/X11R6/lib/ -lqt-mt -lXext -lX11 -lm -lpthread
AR       = ar cqs
RANLIB   = 
MOC      = $(QTDIR)/bin/moc
UIC      = $(QTDIR)/bin/uic
QMAKE    = qmake
TAR      = tar -cf
GZIP     = gzip -9f
COPY     = cp -f
COPY_FILE= $(COPY)
COPY_DIR = $(COPY) -r
INSTALL_FILE= $(COPY_FILE)
INSTALL_DIR = $(COPY_DIR)
DEL_FILE = rm -f
SYMLINK  = ln -sf
DEL_DIR  = rmdir
MOVE     = mv -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p

####### Output directory

OBJECTS_DIR = ./

####### Files

HEADERS = src/debugmsg.h \
		src/loadConfig.h \
		src/tbmainwindowimpl.h \
		src/tbSearchEngine.h \
		src/torrentSearch.h \
		resultsParser.h \
		searchResult.h
SOURCES = src/tbmainwindowimpl.cpp \
		src/torrentbubbles.cpp
OBJECTS = tbmainwindowimpl.o \
		torrentbubbles.o \
		tbmainwindow.o
FORMS = src/tbmainwindow.ui
UICDECLS = src/tbmainwindow.h
UICIMPLS = src/tbmainwindow.cpp
SRCMOC   = src/moc_tbmainwindowimpl.cpp \
		src/moc_tbmainwindow.cpp
OBJMOC = moc_tbmainwindowimpl.o \
		moc_tbmainwindow.o
DIST	   = torrentbubbles.pro
QMAKE_TARGET = torrentbubbles
DESTDIR  = bin/
TARGET   = bin/torrentbubbles

first: all
####### Implicit rules

.SUFFIXES: .c .o .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	test -d bin/ || mkdir -p bin/
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(OBJCOMP) $(LIBS)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: torrentbubbles.pro  /usr/lib/qt3/mkspecs/default/qmake.conf /usr/lib/qt3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile torrentbubbles.pro
qmake: 
	@$(QMAKE) -o Makefile torrentbubbles.pro

dist: 
	@mkdir -p .tmp/torrentbubbles && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/torrentbubbles/ && ( cd `dirname .tmp/torrentbubbles` && $(TAR) torrentbubbles.tar torrentbubbles && $(GZIP) torrentbubbles.tar ) && $(MOVE) `dirname .tmp/torrentbubbles`/torrentbubbles.tar.gz . && $(DEL_FILE) -r .tmp/torrentbubbles

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
clean: mocclean uiclean
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) bin/$(TARGET) $(TARGET)


FORCE:

####### Compile

tbmainwindowimpl.o: src/tbmainwindowimpl.cpp src/tbmainwindowimpl.h \
		src/debugmsg.h \
		src/loadConfig.h \
		src/tbSearchEngine.h \
		resultsParser.h \
		tbmainwindow.h \
		src/torrentSearch.h \
		searchResult.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o tbmainwindowimpl.o src/tbmainwindowimpl.cpp

torrentbubbles.o: src/torrentbubbles.cpp src/tbmainwindowimpl.h \
		tbmainwindow.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o torrentbubbles.o src/torrentbubbles.cpp

src/tbmainwindow.h: src/tbmainwindow.ui 
	$(UIC) src/tbmainwindow.ui -o src/tbmainwindow.h

src/tbmainwindow.cpp: src/tbmainwindow.h src/tbmainwindow.ui 
	$(UIC) src/tbmainwindow.ui -i tbmainwindow.h -o src/tbmainwindow.cpp

tbmainwindow.o: src/tbmainwindow.cpp src/tbmainwindow.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o tbmainwindow.o src/tbmainwindow.cpp

moc_tbmainwindowimpl.o: src/moc_tbmainwindowimpl.cpp  src/tbmainwindowimpl.h tbmainwindow.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o moc_tbmainwindowimpl.o src/moc_tbmainwindowimpl.cpp

moc_tbmainwindow.o: src/moc_tbmainwindow.cpp  src/tbmainwindow.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o moc_tbmainwindow.o src/moc_tbmainwindow.cpp

src/moc_tbmainwindowimpl.cpp: $(MOC) src/tbmainwindowimpl.h
	$(MOC) src/tbmainwindowimpl.h -o src/moc_tbmainwindowimpl.cpp

src/moc_tbmainwindow.cpp: $(MOC) src/tbmainwindow.h
	$(MOC) src/tbmainwindow.h -o src/moc_tbmainwindow.cpp

####### Install

install:  

uninstall:  

